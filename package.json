    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "peer": "^0.2.10",
    "popper.js": "^1.14.4",
    "sass": "^1.15.2",
    "select2": "^4.0.6-rc.1",
    "select2-bootstrap4-theme": "^1.0.0",
    "simple-line-icons": "^2.4.1",
    "vee-validate": "^2.1.5",
    "vue": "^2.5.17",
